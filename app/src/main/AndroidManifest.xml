<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hospital">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@mipmap/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon"
        android:supportsRtl="true"

        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:usesCleartextTraffic"
        tools:targetApi="m">
        <activity android:name=".payment.MyPayments"></activity>
        <activity android:name=".aboutUs.Testimonials" />
        <activity android:name=".payment.MakePayment" />
        <activity android:name=".ShoppingMain" />
        <activity android:name=".cart.OldOrders.OldOrders" />
        <activity android:name=".cart.orders.YourOrders" />
        <activity android:name=".Register.OtpView" />
        <activity android:name=".profile.Profile" />
        <activity android:name=".aboutUs.BlogView" />
        <activity android:name=".AddressDetails" />
        <activity
            android:name=".Register.Login"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".cart.ViewProduct" />
        <activity android:name=".cart.Cart" />
        <activity android:name=".aboutUs.Blog" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".AllShoes.AllShoes"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Appointment.Appointment"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Shop.Sopping"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Appointment.BookApponement"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aboutUs.AboutUs" />
        <activity android:name=".payment.MerchantCheckoutActivity" />
    </application>

</manifest>